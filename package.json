{
  "name": "ccat-api",
  "description": "API Client to communicate with the Cheshire Cat AI",
  "license": "MIT",
  "version": "0.2.1",
  "author": "zAlweNy26",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "keywords": [
    "ccat",
    "cheshire-cat",
    "api",
    "client",
    "fastapi",
    "openapi",
    "ai",
    "llm",
    "chat",
    "bot"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zAlweNy26/ccat-api.git"
  },
  "bugs": {
    "url": "https://github.com/zAlweNy26/ccat-api/issues"
  },
  "homepage": "https://github.com/zAlweNy26/ccat-api",
  "scripts": {
    "build": "tsup index.ts --format cjs,esm --dts",
    "release": "pnpm run build && changeset publish",
    "lint": "tsc",
    "start": "ts-node index.ts",
    "generate": "openapi --input ./catapi.json --output ./api --client axios --name CCatAPI --useUnionTypes --useOptions"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@types/node": "^20.3.1",
    "openapi-typescript-codegen": "^0.24.0",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "form-data": "^4.0.0",
    "isomorphic-ws": "^5.0.0",
    "ws": "^8.13.0"
  }
}
