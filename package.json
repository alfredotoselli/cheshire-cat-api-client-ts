{
  "name": "ccat-api",
  "license": "MIT",
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup index.ts --format cjs,esm --dts",
    "release": "pnpm run build && changeset publish",
    "lint": "tsc",
    "start": "ts-node index.ts",
    "generate": "openapi --input ./catapi.json --output ./api --client axios --name CCatAPI --useUnionTypes  --useOptions"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@types/node": "^20.3.1",
    "openapi-typescript-codegen": "^0.24.0",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "form-data": "^4.0.0",
    "isomorphic-ws": "^5.0.0",
    "ws": "^8.13.0"
  }
}
